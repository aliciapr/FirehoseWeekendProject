
<h2 class="custom-pic-header">Here are all the pics!</h2>

	<% @pics.each do |pic| %>

	<div class="span3">
		<div class="boxy">
			<h2>Hour: <%= pic.hour %></h2>
			<p />
			<h4>Emotion:
				<p />
				<%= pic.emotion %>
			</h4>
			<p />
				<%= image_tag pic.image %>
			<br />
			<h4>What I learned: <p /></h4>
				<%= pic.learned %>
			<p />
		</div>
	</div>

	<% end %>
	<br />